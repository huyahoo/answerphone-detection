{
  "name": "answerphone-detection",
  "version": "1.0.0",
  "description": "Convert binary audio data to WAV and transcribe to text",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "wav": "node scripts/binaryDataToWav.js",
    "text": "node scripts/speechToText.js",
    "batch": "node index.js --folder",
    "export-csv": "node index.js --folder data/20250702 --export-csv",
    "single": "node index.js --file"
  },
  "keywords": [
    "answering-machine",
    "speech-recognition", 
    "audio-processing",
    "native-javascript"
  ],
  "author": "Huy",
  "dependencies": {
    "@google-cloud/speech": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 